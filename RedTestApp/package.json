{
  "name": "RedTestApp",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint .",
    "web": "NODE_ENV=development webpack-dev-server -d --config web/webpack.config.js --inline --colors --devtool source-map",
    "android": "react-native run-android",
