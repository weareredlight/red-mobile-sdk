    "babel-loader": "^8.1.0",
    "babel-plugin-module-resolver": "^4.0.0",
    "lerna": "^3.21.0",
    "prop-types": "^15.7.2",
    "react": "^16.11.0",
    "react-native": "^0.62.2",
    "webpack": "^4.43.0"
  }
